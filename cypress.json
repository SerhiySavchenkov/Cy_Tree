{
    "ignoreTestFiles": "**/data/*.js",
    "viewportWidth": 1200,
    "reporter": "cypress-multi-reporters",
    "reporterOptions": {
        "reporterEnabled": "mochawesome",
        "mochawesomeReporterOptions": {
            "reportDir": "cypress/reports/mocha",
            "quite": true,
            "overwrite": false,
            "html": false,
            "json": true
        },
        "screenshotsFolder": "cypress/report/mochawesome-report/assets"
    }
}
