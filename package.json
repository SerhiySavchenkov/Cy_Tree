{
  "name": "Cy_Tree",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "cypress:open": "cypress open",
    "clean": "rm -r -f cypress/reports && mkdir cypress/reports && mkdir cypress/reports/mochareports",
   
    "scripts": "cypress run",
    "combine:reports": "mochawesome-merge --reportDir cypress/reports/mocha > cypress/reports/mochareports/report.json",
    "generate:report": "marge cypress/reports/mochareports/report.json -f report -o cypress/reports/mochareports",
    "e2e": "npm run scripts && npm run combine:reports && npm run generate:report",
    "report": "npm run combine:reports && npm run generate:report"
  },
  "devDependencies": {
    "cypress": "^3.8.0",
    "cypress-multi-reporters": "^1.2.3",
    "mocha": "^5.2.0",
    "mochawesome": "^4.1.0",
    "mochawesome-merge": "^2.0.1",
    "mochawesome-report-generator": "^4.0.1"
  }
}
